{
	"info": {
		"_postman_id": "c2bd6887-611b-4ee1-a455-8c81987f4c51",
		"name": "PetClinic_Swagger",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "PCPetVisits",
			"item": [
				{
					"name": "Delete visits",
					"item": [
						{
							"name": "Delete visits",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"pm.test(\"Delete pet visit Status code is 204\", function () {\r",
											"    pm.response.to.have.status(204);\r",
											"});\r",
											"\r",
											"var url = pm.environment.get(\"url\") + \"/visits/\" +  pm.globals.get(\"visitId\");\r",
											"\r",
											"const getRequest = {\r",
											"        url: url,\r",
											"        method: 'GET'\r",
											"};\r",
											"pm.sendRequest(getRequest, function (err, res) {\r",
											"    if (err) { \r",
											"        console.log(err);\r",
											"    }\r",
											"     console.log(res);\r",
											"    \r",
											"    pm.test('Visit Response Should Be Not Found', function () {\r",
											"        pm.expect(res.status).to.include(\"Not Found\");\r",
											"    });\r",
											"    \r",
											"    pm.test(\"Visit Response Code 404\", function() {\r",
											"        pm.expect(res.code).to.equal(404);\r",
											"    })\r",
											"});"
										],
										"type": "text/javascript"
									}
								}
							],
							"request": {
								"method": "DELETE",
								"header": [],
								"url": {
									"raw": "{{url}}/api/visits/{{visitId}}",
									"host": [
										"{{url}}"
									],
									"path": [
										"api",
										"visits",
										"{{visitId}}"
									]
								}
							},
							"response": []
						}
					]
				}
			]
		}
	]
}